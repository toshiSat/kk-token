  "main": "index.js",
  "scripts": {
    "compile": "yarn hardhat compile",
    "console": "yarn hardhat console",
    "coverage": "yarn hardhat coverage",
    "deploy": "yarn hardhat run scripts/deploy.ts",
    "lint": "yarn prettier && yarn lint:ts && yarn lint:solidity",
    "lint:ts": "yarn eslint '**/*.{js,ts}'",
    "lint:solidity": "yarn solhint src/**/*.sol",
    "lint:fix": "yarn solhint src/**/*.sol --fix && yarn eslint '**/*.{js,ts}' --fix",
    "prettier": "prettier '**/*.{json,sol,md}' --write",
    "clean": "rm -rf node_modules artifacts cache typechain-types deployments",
    "test": "yarn hardhat test",
    "local-node": "dotenv cross-var yarn hardhat node --fork %MAINNET_URL%"
  },
